	   public void Brcr2() throws InterruptedException
		{
		 
		   Brnam.sendKeys("fddsfsdfdds");
		   add1.sendKeys("#fdfdfff556");
		   Zpcde.sendKeys("45543");
		   cntry.sendKeys("INDIA");
		   Thread.sleep(2000);
		   sate.sendKeys("Delhi");
		   Thread.sleep(2000);
		   crty.sendKeys("Delhi");
		   sbm.click();
		   
		   
		   
		   

		}
	   
